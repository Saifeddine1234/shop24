                        <div class="rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="product__details__price"><%=produits.prix %> DT</div>
                        <b>catégorie: </b>  <%=produits.categorie %> <br>
                        <b>sous-catégorie: </b> <%=produits.souscategorie %><br>
